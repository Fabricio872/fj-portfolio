<a href="{{ webProject.url }}" target="_blank" class="card m-2" style="width: 18rem">
    <div class="position-absolute top-50 start-50 translate-middle">
        <div class="spinner-border" role="status">
            <span class="visually-hidden">Loading...</span>
        </div>
    </div>
    <img loading="lazy" src="{{ path('image.web-project', {'id': webProject.id}) }}"
         class="image position-relative" alt="">
    <div class="card-body">
        <h5 class="card-title">{{ webProject.title }}</h5>
        <p class="card-text">{{ webProject.description }}</p>
    </div>
</a>
